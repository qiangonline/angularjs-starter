{
  "name": "angularjs-webpack2-example",
  "version": "1.0.0",
  "description": "angularjs(ng1) webpack2",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && webpack --config ./config/webpack.build.config.js --progress",
    "dev": "webpack-dev-server --config ./config/webpack.dev.config.js --progress",
    "test": "karma start ./config/karma.conf.js",
    "e2e": "npm-run-all -p -r dev webdriver:start protractor:delay",
    "lint": "eslint ./src",
    "clean": "rimraf dist",
    "postinstall": "npm run webdriver:update",
    "protractor:delay": "sleep 1 && protractor ./config/protractor.conf.js",
    "webdriver:start": "webdriver-manager start",
    "webdriver:update": "webdriver-manager update"
  },
  "keywords": [
    "angular",
    "webpack2",
    "karma",
    "jasmine",
    "protractor"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "angular": "^1.5.11",
    "angular-route": "^1.5.11",
    "angular-ui-router": "^0.4.2",
    "babel-polyfill": "^6.23.0",
    "oclazyload": "^1.0.9"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.11",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.26.1",
    "eslint": "^2.10.2",
    "eslint-loader": "^1.3.0",
    "estraverse-fb": "^1.3.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.10.0",
    "isparta": "^4.0.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.2",
    "ng-annotate-loader": "^0.2.0",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.1",
    "protractor": "^5.1.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^6.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webdriver-manager": "^12.0.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
}